{
  "name": "barscrap",
  "version": "0.3.0",
  "description": "Scrapper de productos basado en códigos de barras",
  "author": "Marce Concepción",
  "bin": {
    "barscrap": "./bin/run"
  },
  "homepage": "https://github.com/cmo7/barscrap",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "cmo7/barscrap",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^2.8.5",
    "@oclif/plugin-help": "^5.2.9",
    "@oclif/plugin-plugins": "^2.4.7",
    "chalk": "^4.1.2",
    "csv": "^6.2.12",
    "puppeteer": "^20.1.1",
    "sqlite": "^4.2.0",
    "sqlite3": "^5.1.6"
  },
  "devDependencies": {
    "@oclif/test": "^2.3.17",
    "@types/async-csv": "^2.1.1",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.18.25",
    "chai": "^4.3.7",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4.0.0",
    "eslint-config-oclif-typescript": "^1.0.3",
    "mocha": "^9.2.2",
    "oclif": "^3.9.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "barscrap",
    "dirname": "barscrap",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "package": "oclif pack win && oclif pack macos"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/cmo7/barscrap/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
